{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenySensitiveTagRemoval",
      "Effect": "Deny",
      "Action": [
        "s3:DeleteObjectTagging",
        "s3:PutObjectTagging"
      ],
      "Resource": "arn:aws:s3:::*/*",
      "Condition": {
        "StringEquals": {
          "aws:RequestTag/Sensitive": "false"
        }
      }
    }
  ]
}
